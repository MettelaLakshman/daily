class Solution {
public:
    bool canSortArray(vector<int>& nums) {
        if(is_sorted(nums.begin(),nums.end())) return true;
        int n=nums.size();
        int swap=0;
        while(1){
            swap=0;
            for(int i=0;i<n-1;i++){
                if(nums[i]>nums[i+1] and (__builtin_popcount(nums[i])==__builtin_popcount(nums[i+1]))){
                    int t=nums[i+1];
                    nums[i+1]=nums[i];
                    nums[i]=t;
                    swap++;
                }
            }
            if(swap==0) break;
        }
        if(is_sorted(nums.begin(),nums.end())) return true;
        returnÂ false;

        
    }
};
