#include<bits/stdc++.h>
using namespace std;
int main()
{
    int x;
    cin>>x;
    while(x--)
    {
        int a;
        cin>>a;
        vector<int>vt1(a);
        for(int i=0;i<a;i++)
        {
            cin>>vt1[i];
        }
        int n=10-a,r=2,n1=1;
        for(int i=n-r+1;i<=n;i++)
        {
            n1=n1*i;
        }
        n1=n1/2;
        cout<<n1*6<<"\n";
    }
}
